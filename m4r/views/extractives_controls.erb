      <li><a href="#" onclick="wb.setExtractiveIndicator('Oil wells','Lift total','Production',true); return false">Production</a></li>
      <li><a href="#" onclick="wb.setExtractiveIndicator('Oil wells','Sale total','Oil sales',true); return false">Oil sales</a></li>
