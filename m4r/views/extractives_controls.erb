  <li><a href="#" id="layercontrol_extractives" class="arwOrange" >Mines Production</a>
    <ul class="dropmenu_options">
      <li class="dropmenu_category">Mines</li>
      <li><a href="#" onclick="wb.setExtractiveIndicator('Mines','Total production','Production',true); return false">Production</a></li>
      <li><a href="#" onclick="wb.setExtractiveIndicator('Mines','Total company payments','Company Payments',true); return false">Company Payments</a></li>
      <li><a href="#" onclick="wb.setExtractiveIndicator('Mines','Total government receipts','Government Receipts',true); return false">Government Receipts</a></li>
